    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/Content/Stylecss.css">
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
</head>
<body>
    <div class="container">
        <!-- Logo -->
        <div class="logo">
            <a href="#"><img src="~/Content/Image/Booking.Com-logo.png" alt="Booking Logo"></a>
        </div>
        <!-- Navigation Link -->
        <nav class="nav">
            <ul>
                <li><a href="#">Máy bay + Khách sạn </a></li>
                <li><a href="#">Phương tiện di chuyển</a></li>
                <li><a href="#">Hoạt động</a></li>
                <li><a href="#">Các tiện ích của khách sạn </a></li>
                <li><a href="#">Phiếu giảm giá và ưu đãi</a></li>
            </ul>
        </nav>

        <!-- Search Bar -->
        <div class="search-bar">
            <input type="text" placeholder="Nhập điểm du lịch và tên khách sạn">
            <button type="button">Search</button>
        </div>

        <!-- User Options -->
        <div class="user-options">
            <a href="#" class="login">Login</a>
            <a href="#" class="signup">Sign Up</a>
        </div>
    </div>

     @RenderBody();


     <@*    @Scripts.Render("~/bundles/jquery")
         @Scripts.Render("~/bundles/bootstrap")
         @RenderSection("scripts", required: false)*@
</body>
